<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Enable lockscreen translucent decor by default -->
    <bool name="config_enableLockScreenTranslucentDecor">true</bool>

    <!-- Component name of the default wallpaper. This will be ImageWallpaper if not specified -->
    <string name="default_wallpaper_component" translatable="false">com.android.phasebeamorange/.PhaseBeamWallpaper</string>

    <!-- Package name for default keyguard appwidget [DO NOT TRANSLATE] -->
    <string name="widget_default_package_name">com.dvtonder.chronus</string>

    <!-- Class name for default keyguard appwidget [DO NOT TRANSLATE] -->
    <string name="widget_default_class_name">com.dvtonder.chronus.ClockWidgetProvider</string>	
</resources>
